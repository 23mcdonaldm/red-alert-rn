{
  "expo": {
    "name": "redalertapp",
    "slug": "redalertapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/omal-logo-o.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.redalert.redalertapp",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/omal-logo-o.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.redalert.redalertapp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/omal-logo-o.png"
    },
    "splash": {
      "image": "./assets/images/omal-logo-o.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/omal-logo-o.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "611a35e3-5418-4733-a617-9e28b8e54858"
      }
    }
  }
}
